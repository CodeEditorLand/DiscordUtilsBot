{
  "compilerOptions": {
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "bundler",

    "target": "esnext",

    "types": ["@cloudflare/workers-types"],
    "esModuleInterop": true,
    "checkJs": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,

    "noEmit": true,
    "verbatimModuleSyntax": true
  }
}
